{
  "name": "angularcg",
  "displayName": "angularcg",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "angularcg.openConfig",
        "title": "angularcg: openConfig"
      }
    ]
  },
  "scripts": {
